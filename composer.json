{
    "name": "gallu/slim-littie-tools",
    "description": "little tools for Slim Framework skeleton applications",
    "keywords": ["microframework", "rest", "psr7", "slim"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michiaki Furusho"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/csrf": "^0.8.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "SlimLittleTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlimLittleTools\\Tests\\": "tests/"
        }
    }
}
